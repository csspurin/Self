{
  "name": "project",
  "version": "0.1.0",
  "description": "SASS compile|autoprefix|minimize and live-reload dev server using Browsersync for static HTML",
  "main": "assets/index.html",
  "author": "5t3ph",
  "scripts": {
    "watch:pug": "pug -w -o ./ pug/main/ .pug  -P -E php",
    "build:sass": "sass  --no-source-map sass:assets/css",
    "watch:sass": "sass  --no-source-map --watch sass:assets/css",
    "watch:js": "webpack --mode development --watch",
    "watch": "npm-run-all --parallel watch:*",
    "build": "npm-run-all build:*",
    "start": "webpack --mode production",
    "postbuild": "postcss assets/css/*.css -u autoprefixer cssnano -r --no-map"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^8.0.6",
    "core-js": "^3.33.2",
    "css-loader": "^6.8.1",
    "mini-css-extract-plugin": "^2.7.6",
    "node": "^12.13.0",
    "postcss-loader": "^7.3.3",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.2.4",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "aos": "^2.3.4",
    "axios": "^1.7.2",
    "bootstrap": "^5.3.2",
    "browser-sync": "^3.0.2",
    "copyfiles": "^2.4.1",
    "cssnano": "^5.0.17",
    "gsap": "^3.12.5",
    "loader-utils": "^3.2.1",
    "node-gyp": "^9.3.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "photoswipe": "^5.4.2",
    "postcss": "^8.4.16",
    "postcss-cli": "^9.1.0",
    "pug": "^3.0.2",
    "radium": "^0.26.2",
    "scroll-out": "^2.2.12",
    "simple-parallax-js": "^5.6.2",
    "splitting": "^1.1.0",
    "swiper": "^11.0.4",
    "vanilla-lazyload": "^17.8.5",
    "webpack-obfuscator": "^0.6.2"
  },
  "overrides": {
    "qs@<6.8.1": "6.11.0",
    "ajv@<6.12.4": "8.11.2",
    "loader-utils@1.4.2": "3.2.1"
  }
}
